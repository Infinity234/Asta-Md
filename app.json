{
  "name": "Asta Md Whatsapp Bot",
  "description": " Whatsapp bot.",
  "keywords": [
    "whatsapp bot"
  ],
  "repository": "https://github.com/Astropeda/Asta-Md",
  "stack": "container",
  "env": {
    "DATABASE_URL": {
      "description": "MONGODB URI",
      "required": false,
      "value": ""
    },
    "SESSION_ID": {
      "description": "SESSION ID",
      "required": true,
      "value": ""
    },
    "MODE": {
      "description": "PRIVATE OR PUBLIC",
      "required": true,
      "value": "private"
    },
    "PREFIX": {
      "description": "PUT ANY (!,*/$) DON'T PUT @",
      "required": false,
      "value": ""
    },
     "HKEY": {
      "description": "Put your Heroku api key Here , Get one from here https://dashboard.heroku.com/account",
      "required": true,
      "value": ""
    },
     "HAPP": {
      "description": "Put the Heroku app name, same as above entered",
      "required": true,
      "value": ""
    },
    "statusview": {
      "description": "TRUE OR FALSE TO VIEW STATUS",
      "required": false,
      "value": ""
    },
    "autoRead": {
      "description": "TRUE OR FALSE TO READ YOUR CHATS",
      "required": false,
      "value": ""
    },
    "antidelete": {
      "description": "TRUE OR FALSE TO FOWARD DELETED MESSAGES",
      "required": false,
      "value": "false"
    },
     "REMOVEBG_KEY": {
      "description": "REMOVE BG API KEY HERE",
      "required": false,
      "value": ""
    }
    },
    "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/DuckyTeam/heroku-buildpack-imagemagick.git"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "https://github.com/clhuang/heroku-buildpack-webp-binaries.git"
    }
  ]
}

